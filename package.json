{
  "name": "fs-nextra",
  "version": "0.5.0",
  "description": "Node.js fs next-gen extra (nextra) methods.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "test": "ava --timeout=20s",
    "test:lint": "eslint --ext ts src test",
    "test:coverage": "npx nyc check-coverage --lines 90 --functions 90 --branches 90",
    "coverage": "npx nyc --require ts-node/register --require source-map-support/register npm test",
    "coverage:report": "npx nyc report --reporter=html",
    "docs": "typedoc --excludeNotExported --exclude ./src/utils/**/*.ts --mode file --out ./dist/ ./src/",
    "lint": "eslint --fix --ext ts src test"
  },
  "keywords": [
    "atomics",
    "compression",
    "copy",
    "delete",
    "directory",
    "file",
    "file system",
    "fs",
    "gz",
    "gzip",
    "json",
    "move",
    "native",
    "nextra",
    "promise",
    "read",
    "tar",
    "write"
  ],
  "author": "BDISTIN",
  "license": "MIT",
  "engines": {
    "node": ">=12.12.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^13.9.8",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "ava": "^3.8.2",
    "eslint": "^6.8.0",
    "eslint-config-klasa": "dirigeants/klasa-lint",
    "nyc": "^15.0.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bdistin/fs-nextra"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*.ts",
      "!test/lib"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  }
}
