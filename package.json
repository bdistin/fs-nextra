{
  "name": "fs-nextra",
  "version": "0.3.0",
  "description": "Node.js V8 native fs enhanced with util.promisify and standard extra methods.",
  "main": "src/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "test": "npx ava",
	"test:lint": "npx eslint src test && npx tslint 'typings/*.ts'",
	"test:coverage": "npx run coverage && npx nyc check-coverage --lines 50 --functions 50 --branches 50",
	"coverage": "npx nyc npm test",
	"coveralls": "npx nyc report --reporter=text-lcov | coveralls",
	"docs": "npx jsdoc -c ./.docstrap.json -R README.md",
	"lint": "npx eslint --fix src test && npx tslint --fix 'typings/*.ts'"
  },
  "keywords": [
    "atomics",
    "copy",
    "delete",
    "directory",
    "file",
    "file system",
    "fs",
    "json",
    "move",
    "native",
    "nextra",
    "promise",
    "read",
    "write"
  ],
  "author": "BDISTIN",
  "license": "MIT",
  "engines": {
    "node": ">=8.5.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.6.1",
    "ink-docstrap": "github:bdistin/docstrap",
    "jsdoc": "github:jsdoc3/jsdoc",
    "mock-fs": "^4.4.1",
    "nyc": "^11.2.1",
    "tslint": "^5.7.0",
    "tsubaki": "^1.2.0",
    "typescript": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bdistin/fs-nextra"
  }
}
