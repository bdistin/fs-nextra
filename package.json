{
  "name": "fs-nextra",
  "version": "0.4.0",
  "description": "Node.js native fs promises with next-generation extra methods.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "yarn build && ava test/*.ts",
    "test:lint": "tslint '{test,src}/**/*.ts'",
    "test:coverage": "coverage && nyc check-coverage --lines 60 --functions 60 --branches 60",
    "coverage": "nyc yarn test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "docs": "typedoc --mode file --out ./docs/ ./src/",
    "lint": "tslint --fix '{test,src}/**/*.ts'"
  },
  "keywords": [
    "atomics",
    "copy",
    "delete",
    "directory",
    "file",
    "file system",
    "fs",
    "json",
    "move",
    "native",
    "nextra",
    "promise",
    "read",
    "write"
  ],
  "author": "BDISTIN",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.1.3",
    "ava": "^1.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.2.0",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "nyc": "^13.0.1",
    "tslint": "^5.7.0",
    "ts-node": "8.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bdistin/fs-nextra"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
