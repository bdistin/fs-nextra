{
  "name": "fs-nextra",
  "version": "0.4.5",
  "description": "Node.js native fs promises with next-generation extra methods.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "test": "yarn build && ava --tap | tap-xunit --package=\"fs-nextra\" > test-results.xml",
    "test:lint": "eslint --ext ts src test",
    "test:coverage": "yarn coverage && nyc check-coverage --lines 90 --functions 90 --branches 90",
    "coverage": "npx nyc npm test",
    "coverage:azure": "npx nyc report --reporter=cobertura",
    "docs": "typedoc --excludeNotExported --exclude ./src/utils/**/*.ts --mode file --out ./dist/ ./src/",
    "lint": "eslint --fix --ext ts src test"
  },
  "keywords": [
    "atomics",
    "compression",
    "copy",
    "delete",
    "directory",
    "file",
    "file system",
    "fs",
    "gz",
    "gzip",
    "json",
    "move",
    "native",
    "nextra",
    "promise",
    "read",
    "tar",
    "write"
  ],
  "author": "BDISTIN",
  "license": "MIT",
  "engines": {
    "node": ">=10.1.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "ava": "^2.1.0",
    "eslint": "^5.2.0",
    "eslint-config-klasa": "dirigeants/klasa-lint",
    "nyc": "^14.0.0",
    "tap-xunit": "^2.4.1",
    "ts-node": "^8.0.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bdistin/fs-nextra"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*.ts",
      "!test/lib"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
